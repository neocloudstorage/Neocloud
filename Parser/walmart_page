function setPreferredStore() {
if (typeof BrowserPreference.PREFCITY != "undefined") {
// replace "Find your local store" with city of preferred store
document.getElementById("com.wm.module.305705.search").innerHTML =
"Your Store: <br><a href='/storeLocator/ca_storefinder_details_short.do?edit_object_id="
+ BrowserPreference.PREFSTORE
+ "'>"
+ BrowserPreference.PREFCITY
+ "<\/a>";
document.getElementById("com.wm.module.305705.store").style.display = "block";
}
