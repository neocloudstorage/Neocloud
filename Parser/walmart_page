if (!WALMART.bubbleReminder || typeof WALMART.bubbleReminder !== 'object') {
var customerId, wmtItem, count, getBubbleUrl;
var ID_BUBBLE="#wmReminderBubble"
,ID_CLOSE="#wmReminderItemClose"
,ID_TITLE="#wmReminderItemTitle"
,ID_IMAGE="#wmReminderItemImage"
,ID_IMAGEURL="#wmReminderItemImageUrl"
,ID_NAME="#wmReminderItemName"
,ID_URL="#wmReminderItemUrl"
,ID_PRICE="#wmReminderItemPrice"
,ID_SEEALL ="#wmSeeAllTitle"
,ID_TOTALCOUNT="#wmReminderTotalCount"
,IS_COUNT_SET = false;
function handleError(jqXHR, textStatus, errorThrown){
}
function handleSuccess(data){
BrowserPreference.refresh();
if (data!=null && typeof data != "undefined"){
if(isNumber(data.TotalReminderItems)){
BrowserPreference.updateSessionCookie('RC', data.TotalReminderItems+"");
}
if (BrowserPreference.BUBBLE && BrowserPreference.BUBBLE != "NO"){
if (data.ReminderType == "ORDER_AGAIN"){
WALMART.$(ID_TITLE).html("Time to reorder?");
WALMART.$(ID_SEEALL).html("See more");
WALMART.$(ID_TOTALCOUNT).html("Suggested Items ("+data.TotalSuggestedItems+")");
}else if(data.ReminderType == "REMINDER"){
WALMART.$(ID_TITLE).html("Remind Me Item:");
WALMART.$(ID_SEEALL).html("See all");
WALMART.$(ID_TOTALCOUNT).html("Remind Me List items ("+data.TotalReminderItems+")");
}
if (data.isSubmapPricing){
var protocol = 'http';
if('https' == protocol){
var seeOurPriceHtml = '<a id="see_our_price_'+ data.itemId +'" href="javascript:WALMART.productservices.submapPricingOverlay.openSubmapOverlay(\'' + data.itemId +'\', 1, false);" class="SubMapLink">See Our Price</a>';
}else{
var seeOurPriceHtml = '<a id="see_our_price_'+ data.itemId +'" href="javascript:popWindow(\'/catalog/submapPricingPopup.do?product_id=' + data.itemId +'\', \'SMpopup\', 550, 500, \'no\');" class="SubMapLink">See Our Price</a>';
}
WALMART.$(ID_PRICE).html(seeOurPriceHtml);
}else if (Number(data.itemDisplayPrice)>0){
WALMART.$(ID_PRICE).html("$"+data.itemDisplayPrice);
}
WALMART.$(ID_IMAGE).attr("src", data.imageThumbnail);
WALMART.$(ID_IMAGEURL).attr("href", ""+data.canonicalUrl+"?findingMethod=rl");
WALMART.$(ID_URL).attr("href", ""+data.canonicalUrl+"?findingMethod=rl");
WALMART.$(ID_URL).html(data.itemName);
WALMART.$(ID_BUBBLE).wmBubble().wmBubble('showFromTrigger');
}
}
BrowserPreference.refresh();
WALMART.bubbleReminder.updateReminderCount();
}
function isStoreGeoLookupNeeded(){
return WALMART.page.isPreferredStoreNeeded && !BrowserPreference.GEOLOOKUPDONE && !BrowserPreference.PREFSTORE;
}
function isNumber(n) {
return !isNaN(parseFloat(n)) && isFinite(n);
}
WALMART.jQuery(document).bind("menuOpen", function (event, menuId) {
WALMART.$(ID_BUBBLE).wmBubble('hide');
});
// Make a call only if customerId is available and BUBBLE cookie does not exists
WALMART.bubbleReminder = {
init:function(getReminderBubbleDataUrl){
if (getCustomerId()!=null && (!BrowserPreference.BUBBLE) && (!isStoreGeoLookupNeeded())){
WALMART.$.ajax({
dataType: 'jsonp',
data: { customerId : getCustomerId() },
url: getReminderBubbleDataUrl,
success: handleSuccess,
error: handleError
});
}
},
updateReminderCount:function(){
if(getCustomerId()!=null && BrowserPreference.RC){
WALMART.$("#myLists a").each(function(index){
var v = WALMART.$(this).html();
if (v.indexOf("Remind Me List") != -1){
if (BrowserPreference.RC && !IS_COUNT_SET && Number(BrowserPreference.RC)>0){
IS_COUNT_SET = true;
WALMART.$(this).html("Remind Me List" + " ("+BrowserPreference.RC+") ");
}
}
});
}else{
WALMART.$("#myLists a").each(function(index){
var v = WALMART.$(this).html();
if (v.indexOf("Remind Me List") != -1){
WALMART.$(this).html("Remind Me List");
}
});
}
}
}
}
WALMART.bubbleReminder.init("/reminder/getReminderBubbleData.do");
WALMART.bubbleReminder.updateReminderCount();
