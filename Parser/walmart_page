WALMART.$(function($){
var morelink = "[+]";
var lesslink = "[-]";
$("span.showmore").click(function(){
var cr=$(this).text();
$(this).next().next("ul.sublink").toggle()
.parent().siblings().children("ul.sublink").hide();
if (cr == morelink) {
$(this).text(lesslink);
$(this).parents().siblings().children("span.showmore").text(morelink);
} else {
$(this).text(morelink);
}
});
})
