<script type="text/javascript" language="JavaScript">
function sortSearch(sortKeyvalue, existingSelVal) {
var sortElement = document.getElementById("sortValueNew");
if(sortElement) {
sortElement.value = sortKeyvalue;
if(sortElement.value==""){sortElement.value =" ";}
}
document.getElementById("sortBy").submit();
}
</script>
