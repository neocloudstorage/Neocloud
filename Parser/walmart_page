function doLink(target, catNavId, type) {
var querySeparator = "&";
if (target.indexOf("?") == -1) {
querySeparator = "?";
}
if (type == 'E') {
document.location = target;
} else {
if (target.indexOf('catNavId') != -1 || catNavId == null || catNavId == undefined) {
document.location = target + querySeparator + "fromPageCatId=" + 489706;
} else {
document.location = target + querySeparator + "fromPageCatId=" + 489706 + "&catNavId=" + catNavId;
}
}
}
</script>
